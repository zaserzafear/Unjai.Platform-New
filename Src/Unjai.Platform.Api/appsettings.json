{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "UnjaiDb": "Host=localhost;Database=unjai;Username=postgres;Password=1234",
    "UnjaiDbRead": "",
    "UnjaiDbWrite": "",
    "Redis": "localhost:6379"
  },
  "ApiVersions": [
    "1"
  ],
  "ApiKeys": {
    "HealthCheck": ""
  },
  "Jwt": {
    "Issuer": "https://localhost:5001",
    "Audience": "https://localhost:5001",
    "Secret": "",
    "ExpireMinutes": 60,
    "ClockSkew": 30
  },
  "RateLimiting": {
    "Secret": "",
    "ContextTtl": "00:01:00",
    "Policies": {
      "get-user": {
        "Strategy": "FixedWindow",
        "Limit": 60,
        "Window": "00:01:00"
      },
      "login": {
        "Strategy": "SlidingWindow",
        "Limit": 5,
        "Window": "00:01:00"
      },
      "otp": {
        "Strategy": "TokenBucket",
        "Limit": 1,
        "Window": "00:01:00",
        "TokensPerPeriod": 1,
        "ReplenishmentPeriod": "00:01:00"
      }
    }
  }
}
