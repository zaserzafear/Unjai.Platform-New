{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "ApiKeys": {
    "HealthCheck": ""
  },
  "Jwt": {
    "Issuer": "https://localhost:5001",
    "Audience": "https://localhost:5001",
    "Secret": "",
    "ExpireMinutes": 60,
    "ClockSkew": 30
  },
  "RateLimiting": {
    "Policies": {
      "get-user": {
        "limit": 60,
        "window": "00:01:00"
      },
      "login": {
        "limit": 5,
        "window": "00:01:00"
      },
      "otp": {
        "limit": 1,
        "window": "00:01:00"
      }
    }
  },
  "Api": {
    "BaseUrl": "http://localhost:5296"
  }
}
